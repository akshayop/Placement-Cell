<link rel="stylesheet" href="/css/add_interview.css">

<div id="add-interview-container">
    <h2>Interview Details</h2>
    

    <form action="/interview/create-interviews" method="post">
        <div class="input-box">
            <input type="text" name="company_name" required>
            <span>Company Name</span>
        </div>

        <div class="input-box">
            <input type="date" name="ineterview_date" required>
            <span>Interview Date</span>
        </div>

        <input type="submit" value="Add Interview">
    </form>
</div>