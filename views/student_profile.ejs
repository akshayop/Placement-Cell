<div>
    <h2>Student Details</h2>

    <div>
        <div>
            <p>name</p>
            <p><%= student.name %></p>
        </div>
    
        <div>
            <p>Email</p>
            <p><%= student.email %></p>
        </div>
    
        <div>
            <p>Batch</p>
            <p><%= student.batch %></p>
        </div>
    
        <div>
            <p>Status</p>
            <p><%= student.status %></p>
        </div>
    
        <div>
            <p>DSA Marks</p>
            <p><%= student.courseMarks[0] %></p>
        </div>
    
        <div>
            <p>FE Marks</p>
            <p><%= student.courseMarks[1] %></p>
        </div>
    
        <div>
            <p>BE Marks</p>
            <p><%= student.courseMarks[2] %></p>
        </div>
    </div>

    <% if(student.interview) { %>
        <h2>Applied companies</h2>

        <% for(interview of student.interview) { %>

            <div>
                <p>Company Name</p>
                <p><%= interview.company_name%></p>
            </div>

            <div>
                <p>Date</p>
                <p>interview.date</p>
            </div>

            <% for(let interviewResult of interview.students) { %>
                
                <% if(interviewResult == student._id) {%>

                    <div>
                        <p>Result</p>
                        <p><%= interviewResult.result %></p>
                    </div>
                <% } %>
            <% } %>
        <% } %>


    <% } %>
</div>